filebeat.prospectors:

- type: log
  # Paths that should be crawled and fetched. Glob based paths.
  paths:
   - <%= node['filebeat']['read_logs'] %>
  multiline:
    pattern: '[0-9]{4}-[0-9]{2}-[0-9]{2}'
    negate: true
    match: after

output:
  logstash:
    hosts: ["<%= @logstash_endpoint %>"]
